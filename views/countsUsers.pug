extends layout.pug 

block content 

        div(class="container")
            h1 TODOS LOS USUARIOS DE LA ORGANIZACIÓN:
            each user in rows
                div(class="card")
                    h2= user.nombreusuario
                    h3= user.cargousuario
                    h3= user.areausuario 
                    h3= user.estado 
                    h3= user.sucursal
                    h3= user.cuentaOasys 
                    h3= user.contraseñaoasys 
                    h3= user.cuentaSinergia 
                    h3= user.contraseSinergia 
                    h3= user.cuentasCorreo 
                    h3= user.contraseñaCorreo
                    h3= user.usuarioServer 
                    h3= user.contraseñaServer